<%= form_for @user do |f| %>
  YOUR USERNAME:  <%= f.text_field :username ,placeholder: 'example'%>
  <br><br/>

  YOUR EMAIL HERE:  <%= f.email_field :email , placeholder: 'email@gmail.com'%>
  <br><br/>

  CHOOSE PASSWORD:  <%= f.password_field :password %>
  <br><br/>

  <%= f.submit %>
<% end %>